    "test:e2e:headed": "playwright test --headed",
    "test:e2e:all-pages": "playwright test tests/e2e/all-pages.spec.ts",
    "test:report": "node tests/utils/generate-report.js"
  },
  "dependencies": {
    "@craftjs/core": "^0.2.8",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.57.4",
    "@xyflow/react": "^12.9.2",
    "axios": "^1.13.1",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "grapesjs": "^0.21.13",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.3.0",
    "xlsx": "^0.18.5"
